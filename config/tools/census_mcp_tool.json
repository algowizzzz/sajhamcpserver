{
  "name": "census_tool",
  "module": "tools.census_mcp_tool.CensusMCPTool",
  "max_hits": 1000,
  "max_hit_interval": 10,
  "tool_description": {
    "tools": [
      {
        "name": "get_population_data",
        "description": "Get population data from Census",
        "input_schema": {
          "type": "object",
          "properties": {
            "year": {
              "type": "integer",
              "description": "Year of data",
              "default": 2021
            },
            "geography": {
              "type": "string",
              "description": "Geographic level",
              "enum": ["state", "county", "place"],
              "default": "state"
            },
            "state": {
              "type": "string",
              "description": "State FIPS code or * for all",
              "default": "*"
            }
          }
        }
      },
      {
        "name": "get_demographic_data",
